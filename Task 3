#define ARRAY_SIZE 676767
#include <stdio.h>
#include <fcntl.h>
#include <unistd.h>

int main () {
    FILE *fp;

    fp = fopen("/usr/lib/locale/locale-archive", "r");
    if (fp == NULL) {
        perror("Aww man, unable to open file!!\n");
        return 1;
    }

    char buf[ARRAY_SIZE];
    while (fread(buf, 1,ARRAY_SIZE, fp) > 0) {

    }

    fclose(fp);
    return 0;
}
